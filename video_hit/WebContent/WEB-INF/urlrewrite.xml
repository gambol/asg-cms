<?xml version="1.0" encoding="utf-8"?>  
 <!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"  
       "http://tuckey.org/res/dtds/urlrewrite3.1.dtd">  
<urlrewrite>  
    <rule>       
        <from>/index_(.*).htm</from>       
        <to>/index.htm?categoryId=$1</to>        
    </rule>    
     <rule>       
        <from>/detail_(.*).htm</from>       
        <to>/detail.htm?serverId=$1</to>        
    </rule>    
</urlrewrite> 